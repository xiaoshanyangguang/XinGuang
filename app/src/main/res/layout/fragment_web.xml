<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:titlebar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">


    <com.wuhenzhizao.titlebar.widget.CommonTitleBar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        titlebar:centerText="@string/my_plan"
        titlebar:centerType="textView"
        titlebar:rightImageResource="@drawable/rating_small_empty"
        titlebar:rightType="imageButton"
        titlebar:statusBarColor="@color/color_title"
        titlebar:titleBarColor="@color/color_title"/>

    <SeekBar
        android:id="@+id/web_sbr"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:focusable="false"
        android:progressDrawable="@drawable/po_seekbar"
        android:thumb="@null"/>

    <FrameLayout
        android:id="@+id/web_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>